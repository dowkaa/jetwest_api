      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />
    <style>
      @import url("https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700;800&display=swap");
      * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
      }
      body {
        font-family: "Poppins", sans-serif;
        background: #f3f3f3;
      }
      header {
        padding: 20px;
      }
      .hero {
        text-align: center;
      }
      .hero .logo {
        width: 45%;
        padding-top: 40px;
      }
      .main {
        position: relative;
      }
      .main-content {
        padding: 20px;
        padding-top: 20px;
        text-align: center;
        /* height: 100vh; */
      }
      .main-content .title {
        font-size: 18px;
        text-align: center;
        line-height: 28px;
      }
      .main-content .hero-p {
        font-size: 14px;
        padding-top: 10px;
      }
      .main-content .hero-p .welcome-message{
          padding-top: 20px;
          /* text-align: justify; */
      }
      .main-content .link {
        display: inline-block;
        color: #85025a;
        font-size: 14px;
        padding-top: 30px;
      }
      .main-content .sub-info {
        display: block;
        font-size: 13px;
        padding-top: 80px;
        padding-bottom: 5rem;
        /* text-align: justify; */
      }
      .footer {
        margin-top: 20px;
        padding: 20px;
        background: linear-gradient(92.03deg, #c91c6f 28.72%, #a12278 88.13%);
        color: rgba(255, 255, 255, 0.8);
        text-align: center;
        font-size: 14px;
      }
      .footer .unsubscribe a {
        display: inline-block;
        color: rgba(255, 255, 255, 0.8);
        padding-bottom: 10px;
        /* line-height: 5; */
      }
      .social-links {
        margin: 5px 0;
      }
      .social-links .social-icon {
        color: rgba(255, 255, 255, 0.48);
        font-size: 30px;
        margin-left: 20px;
        /* float: left;
		width: 25%; */
      }
      .copyright {
        font-size: 12px;
        clear: both;
      }
      .address {
        line-height: 3;
      }
      @media screen and (min-width: 768px) {
        .hero .logo {
          width: 12%;
          padding-top: 20px;
        }
        .main-content .title {
          font-size: 24px;
          width: 40%;
          margin: 0 auto;
          padding-top: 20px;
        }
        .main-content .hero-p {
          font-size: 16px;
          width: 40%;
          margin: 0 auto;
          padding-top: 30px;
        }
        .main-content .link {
          font-size: 16px;
        }
        .main-content .sub-info {
          width: 45%;
          margin: 0 auto;
          font-size: 15px;
          padding-bottom: 1rem;
        }
        .footer {
          padding-top: 30px;
        }
      }
    </style>
